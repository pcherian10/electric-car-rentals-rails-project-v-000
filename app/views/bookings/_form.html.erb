<% form_for @booking do |f| %>

  <%= f.label :manufacturer, "Select the Car Manufacturer" %><br>
    <%= f.collection_select(:manufacturer, @cars, :id, :manufacturer) %>

  <%= f.label :start_date, "Start Date" %><br>
  <%= f.date_field :start_date %><br>

  <%= f.label :end_date, "End Date" %><br>
  <%= f.date_field :end_date %><br>

  <%= f.submit "Create" %>

<% end %>
